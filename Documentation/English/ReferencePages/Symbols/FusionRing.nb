Cell[TextData[{
 "The format of ",
 StyleBox["multTab",
  FontSlant->"Italic"],
 " is such that ",
 StyleBox["multTab[[a, b, c]]",
  FontFamily->"Courier",
  FontWeight->"Regular"],
 " returns ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["\[ScriptCapitalN]", 
    RowBox[{"a", ",", "b"}], "c"], TraditionalForm]],ExpressionUUID->
  "829903d8-c674-4794-8a28-aafba521f09c"],
 ", with ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["\[ScriptCapitalN]", 
    RowBox[{"a", ",", "b"}], "c"], TraditionalForm]],ExpressionUUID->
  "519823ac-3f20-4da3-b574-194178940480"],
 "  the structure constants of the ring, i.e. those integers for which ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"a", "\[Cross]", "b"}], " ", "=", " ", 
    RowBox[{
     UnderscriptBox["\[Sum]", "c"], 
     RowBox[{
      SubsuperscriptBox["\[ScriptCapitalN]", 
       RowBox[{"a", ",", "b"}], "c"], "  ", "c"}]}]}], TraditionalForm]],
  ExpressionUUID->"dda636d0-bbfd-4d04-a986-f9f55f9a6c87"],
 ". "
}], "Notes",
 CellChangeTimes->{{3.896259147624737*^9, 3.8962591511476507`*^9}},
 CellID->1409960694,ExpressionUUID->"bfb36975-a4de-4f03-a9f9-c48f6b030818"],

